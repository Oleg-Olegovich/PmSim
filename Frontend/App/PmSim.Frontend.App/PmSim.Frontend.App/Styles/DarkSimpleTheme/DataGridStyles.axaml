<Styles xmlns="https://github.com/avaloniaui"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    
    <!-- Add Styles Here -->

    <Style Selector="DataGrid.Common">
        <Setter Property="Foreground" Value="#e3dbd1"/>
        <Setter Property="RowBackground" Value="#898698"/>
        <Setter Property="BorderBrush" Value="#56567a"/>
        <Setter Property="HorizontalGridLinesBrush" Value="#56567a"/>
        <Setter Property="VerticalGridLinesBrush" Value="#56567a"/>
    </Style>
    
    <Style Selector="DataGridColumnHeader">
        <Setter Property="Foreground" Value="#e3dbd1"/>
        <Setter Property="Background" Value="#67678a"/>
    </Style>
</Styles>