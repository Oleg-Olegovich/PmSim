<Styles xmlns="https://github.com/avaloniaui"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    
    <!-- Add Styles Here -->

    <Style Selector="DataGrid.Common">
        <Setter Property="Foreground" Value="#000033"/>
        <Setter Property="RowBackground" Value="#cae099"/>
        <Setter Property="BorderBrush" Value="#879e53"/>
        <Setter Property="HorizontalGridLinesBrush" Value="#879e53"/>
